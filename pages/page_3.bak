<style>
      #map {
        height: 400px;
        width: 735px;
        text-align: right;
       }
    </style>

 <div id="map"></div>
<script>
      function initMap() {
        var lithan = {lat: 1.319681, lng: 103.892044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 15,
          center: lithan
        });
        var marker = new google.maps.Marker({
          position: lithan,
          map: map
        });
      }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB1jY4V_slg-27cc8en9Peat-mgKKH-ZME&callback=initMap">
    </script>

